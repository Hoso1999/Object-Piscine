#example for points
#points
#{
#  (x y) (x1 y1) ...
#}
#example for lines
#lines
#{
#   ((startx starty) (endx endy))
#   ((startx1 starty1) (endx1 endy1)) 
#   ...   
#}
# can be spaces

points
{
    (1.3 5.5) (93.5 7.8) (1.44 10.333)
    (80.3 80.58) (37.5 7.8) (1.44 10.333)
}

lines
{
    (   (0.0 4.5) (1000 1200)   )
    ((100 0) (0 100))
}
